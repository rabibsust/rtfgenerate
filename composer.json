{
    "name": "rabibsust/rtfgenerator",
    "description": "This uses simplertf https://github.com/phprtflite/PHPRtfLite and create rtf documents with laravel",
    "license": "MIT",
    "keywords": ["laravel", "rtf", "generatertf"],
    "authors": [
        {
            "name": "Ahmad Jamaly Rabib",
            "email": "rabib.sust@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x",
        "rabibsust/rtfgenerator": "^0.1"
    },

    "autoload": {
        "psr-4": {
           "Rabibsust\\Rtfgenerator\\": "packages/rabibsust/rtfgenerator/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Rabibsust\\Rtfgenerator\\RtfGenerateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
